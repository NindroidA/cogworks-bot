{
  "name": "cogworks-bot",
  "version": "2.4.0",
  "description": "Discord ticketing bot developed for ease-of-use",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "build": "tsc",
    "dev": "tsc && bun --watch ./dist/src/index.js",
    "dev:ts": "bun --watch ./src/index.ts",
    "dev:node": "nodemon ./src/index.ts",
    "start": "bun run ./src/index.ts",
    "start:node": "tsc && node ./dist/src/index.js",
    "start:compiled": "tsc && bun ./dist/src/index.js"
  },
  "keywords": [],
