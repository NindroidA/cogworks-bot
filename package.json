{
  "name": "cogworks-bot",
  "version": "2.10.2",
  "description": "A modular Discord server management bot built with TypeScript and Discord.js",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "build": "tsc",
    "dev": "tsc && bun --watch ./dist/src/index.js",
    "start": "bun run ./src/index.ts",
    "start:node": "tsc && node ./dist/src/index.js",
    "start:compiled": "tsc && bun ./dist/src/index.js",
    "extract-commands": "bun run scripts/extractCommands.ts",
    "docs:sync": "bun run scripts/extractCommands.ts",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@jest/globals": "^30.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.11",
    "dotenv": "^16.6.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "axios": "^1.13.5",
    "chalk": "^4.1.2",
    "cors": "^2.8.6",
    "discord.js": "^14.25.1",
    "express": "^5.2.1",
    "jszip": "^3.10.1",
    "mysql2": "^3.17.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28"
  }
}
